{
  "name": "contly",
  "version": "1.0.0",
  "description": "",
  "author": "",
  "private": true,
  "type": "module",
  "scripts": {
    "cs:add": "changeset add",
    "cs:version": "changeset version",
    "nm:clean": "pnpm nm:clean:pkgs && pnpm nm:clean:root",
    "nm:clean:root": "pnpm exec rm -rf node_modules",
    "nm:clean:pkgs": "pnpm -r exec rm -rf node_modules",
    "dev": "concurrently -k -n pkg,ts \"pnpm -r --filter=@contly/* run dev\" \"pnpm dev:tsc\"",
    "dev:tsc": "pnpm exec tsc --build --verbose --watch tsconfig.build.json",
    "dev:tscp": "pnpm exec tsc --build --verbose --watch tsconfig.build.json --preserveWatchOutput",
    "pkgs:clean": "pnpm -r run clean",
    "pkgs:build": "pnpm pkgs:clean && turbo run build --filter=@contly/* --no-daemon",
    "pkgs:publish": "pnpm pkgs:build && pnpm publish -r",
    "postinstall": "pnpm exec syncpack format",
    "test": "pnpm -r --filter \"nextjs-*\" --parallel run dev"
  },
  "devDependencies": {
    "@changesets/cli": "2.26.1",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/node": "20.1.4",
    "@typescript-eslint/eslint-plugin": "5.59.6",
    "@typescript-eslint/parser": "5.59.6",
    "concurrently": "8.0.1",
    "eslint": "8.39.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-unicorn": "47.0.0",
    "prettier": "2.8.8",
    "shx": "0.3.4",
    "syncpack": "9.8.6",
    "turbo": "1.9.9",
    "typescript": "5.0.4"
  },
  "syncpack": {
    "dev": true,
    "filter": ".",
    "indent": "  ",
    "overrides": true,
    "peer": true,
    "prod": true,
    "resolutions": true,
    "semverGroups": [],
    "semverRange": "",
    "sortAz": [
      "contributors",
      "dependencies",
      "devDependencies",
      "keywords",
      "peerDependencies"
    ],
    "sortFirst": [
      "name",
      "version",
      "description",
      "author",
      "private",
      "sideEffects",
      "type",
      "main",
      "module",
      "types",
      "files",
      "exports",
      "bin",
      "scripts",
      "dependencies",
      "devDependencies",
      "prettier",
      "xo",
      "eslintConfig",
      "syncpack"
    ],
    "source": [
      "package.json",
      "packages/*/package.json",
      "tests/*/package.json"
    ]
  },
  "keywords": [],
  "license": "ISC"
}
